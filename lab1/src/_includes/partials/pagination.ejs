<nav aria-labelledby="my-pagination">
  <ul class="pagination">
    <%# Previous page button %>
    <% if (pagination.href.previous) { %>
      <li class="page-item">
        <a class="page-link" href="<%= baseUrl %><%= pagination.href.previous %>">Previous</a>
      </li>
    <% } else { %>
      <li class="page-item disabled">
        <a class="page-link">Previous</a>
      </li>
    <% } %>

    
    <%# Page number buttons %>
    <% pagination.pages.forEach((pageEntry, index) => { %>

      <li class="page-item <%= pagination.pageNumber === index ? 'active' : '' %>">
        <a class="page-link" href="<%= baseUrl %><%= pagination.links[index] %>" 
          <% if (pageEntry.url === pagination.pageNumber) { %> aria-current="page" <% } %>>
          Page <%= index + 1 %>
        </a>
      </li>
    <% }); %>

    <%# Next page button %>
    <% if (pagination.href.next) { %>
      <li class="page-item">
        <a class="page-link" href="<%= baseUrl %><%= pagination.href.next %>">Next</a>
      </li>
    <% } else { %>
      <li class="page-item disabled">
        <a class="page-link">Next</a>
      </li>
    <% } %>
  </ul>
</nav>
